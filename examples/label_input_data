import json

with open('intent_for_message/intent_2.json', 'r', encoding='utf-8') as f:
    data = json.load(f)

# print(data)

with open('intent_entities.json', 'r', encoding='utf-8') as f:
    map = json.load(f)

# print(map)

for component in data:
    if component['intent'] == "": 
        continue
    key = component['intent']
    new_label = {}
    for entity in map[key]['entities']:
        new_label[entity] = ""

